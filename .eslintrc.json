{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "html"
    ],
    "parser": "babel-eslint",
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "linebreak-style": "off",
        "no-console": "off"
    }
}
